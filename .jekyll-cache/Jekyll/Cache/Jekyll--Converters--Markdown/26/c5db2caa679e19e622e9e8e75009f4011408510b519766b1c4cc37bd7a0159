<span class="o">}&lt;/</span><span class="n">pre</span><span class="o">&gt;</span>
<span class="nc">And</span> <span class="n">an</span> <span class="n">example</span> <span class="k">for</span> <span class="nl">serialization:</span> <span class="o">&lt;</span><span class="n">pre</span><span class="o">&gt;</span>   <span class="o">{</span><span class="nd">@code</span>
    
